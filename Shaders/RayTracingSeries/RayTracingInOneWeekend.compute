#include "Packages/com.chichi.render-pipelines.too-young/ShaderLibrary/RayTracing/Ray.hlsl"


#pragma kernel RayTracingInOneWeekend

RWTexture2D<float4> _Result;


[numthreads(1,1,1)]
void RayTracingInOneWeekend (uint3 id : SV_DispatchThreadID)
{
    
    _Result[id.xy] = float4(id.x & id.y, (id.x & 15)/15.0, (id.y & 15)/15.0, 0.0);
}
